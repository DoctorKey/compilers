int search(int search_n, int total_n, int a[8]) {
	int i, j, k, flag, flag2=1;
	if (search_n == total_n) {
		k = 0;
		while(k < total_n) {
			write(a[k]);
			k = k + 1;
		}
		return 1;
	}

	i = 1;
	while(i <= total_n) {
		a[search_n] = i;
		flag2 = 1;
		j = 0;
		while( j < search_n) {
			if (a[j] == a[search_n] || (j - search_n) == (a[j] - a[search_n]) || (search_n - j) == (a[j] - a[search_n])) {
				flag2 = 0;
			}
			j = j + 1;
		}
		if (flag2 == 1) {
			flag = search(search_n + 1, total_n, a);
			if (flag == 1) {
				return 1;
			}
		}
		i = i + 1;
	}
}
 
int main(){
	int n = 8;
	int b[8];
	search(0, 8, b);
	return 1;
}
